<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <servlet>
        <servlet-name>StaffRecordPractice</servlet-name>
        <servlet-class>serveletpackages.StaffRecordPractice</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StaffRecord</servlet-name>
        <servlet-class>serveletpackage.StaffRecord</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StaffRecordPractice</servlet-name>
        <url-pattern>/StaffRecordPractice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StaffRecord</servlet-name>
        <url-pattern>/StaffRecord</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        </welcome-file-list>
    <ejb-local-ref>
        <ejb-ref-name>StaffrecordpracticeBean</ejb-ref-name>
        <ejb-ref-type>Entity</ejb-ref-type>
        <local-home>entitypackages.StaffrecordpracticeLocalHome</local-home>
        <local>entitypackages.StaffrecordpracticeLocal</local>
        <ejb-link>StaffRecordsPractice-ejb.jar#StaffrecordpracticeBean</ejb-link>
    </ejb-local-ref>
    <ejb-local-ref>
        <ejb-ref-name>OurLogicBean</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local-home>sessionpackages.OurLogicLocalHome</local-home>
        <local>sessionpackages.OurLogicLocal</local>
        <ejb-link>StaffRecordsPractice-ejb.jar#OurLogicBean</ejb-link>
    </ejb-local-ref>
    </web-app>
